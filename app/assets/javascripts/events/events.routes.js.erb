(function(){
  'use strict';

  angular
    .module('evvntfully.events')
    .config(EventListRoutes)
  ;

  EventListRoutes.$inject = ['$stateProvider'];
  function EventListRoutes ($stateProvider) {
    $stateProvider

      .state('events', {
        url: '',
        templateUrl: "<%= asset_path('events/events.html') %>",
      })

        .state('events.list', {
          url: '/',
          controller: 'EventListController',
          controllerAs: 'events',
          templateUrl: "<%= asset_path('events/events.list.html') %>"
        })
    ;
  }
})();
